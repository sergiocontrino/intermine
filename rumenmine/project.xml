<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="rumenmine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>

  <sources>
    <source name="rnaseq-expression" type="rnaseq-expression">
      <property name="src.data.dir" location="/home/contrino/data/rumen"/>
      <property name="src.data.dir.includes" value="*.txt"/>
      <property name="rnaseq.organisms" value="344734"/>
      <property name="dataSourceName" value="Aber"/>
      <property name="dataSetName" value="Succesional expression"/>
    </source>
    <source name="uniprot-rumen" type="uniprot">
      <property name="uniprot.organisms" value="9913"/>
      <property name="creatego" value="true"/>
      <property name="src.data.dir" location="/home/contrino/data/uniprot/"/>
    </source>
    <source name="successional-gff" type="gff">
      <property name="gff3.taxonId" value="344734"/>
      <property name="gff3.seqDataSourceName" value="Aber"/>
      <property name="gff3.dataSourceName" value="Aber"/>
      <property name="gff3.seqClsName" value="Gene"/>
      <property name="gff3.dataSetTitle" value="Successional Colonisation metatranscriptome"/>
      <property name="src.data.dir" location="/home/contrino/data/rumen"/>
    </source>
    <source name="successional-fasta" type="fasta" >
      <property name="fasta.className" value="org.intermine.model.bio.CDS"/>
      <property name="fasta.dataSourceName" value="Aber"/>
      <property name="fasta.dataSetTitle" value="Unclassified Bacteria CDS sequence"/>
      <property name="fasta.includes" value="*.fna"/>
      <property name="fasta.taxonId" value="344734"/>
      <property name="src.data.dir" location="/home/contrino/data/rumen"/>
    </source>

